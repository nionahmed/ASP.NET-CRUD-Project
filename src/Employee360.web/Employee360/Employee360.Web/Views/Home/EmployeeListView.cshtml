@model List<EmpDataViewModel>;
@{
    ViewData["Title"] = "EmployeeListView";
}

<h1>EmpployeeList</h1>
<table class="table">
    <thead>
        <tr>
            <th scope="col">ID</th>
            <th scope="col">Name</th>
            <th scope="col">Name</th>
        </tr>
    </thead>
    <tbody>
        @foreach(var employee in Model)
        {
            <tr>
                <th scope="row">@employee.Id</th>
                <td><a asp-action="ToEachEmployeeDetails" asp-controller="Home" asp-route-id="@employee.Id">@employee.Name</a></td>
                <td>@employee.Email</td>
            </tr>
        }
       
    </tbody>
</table>

